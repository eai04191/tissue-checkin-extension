{
    "manifest_version": 2,
    "name": "Tissue Checkin",
    "version": "1.1.0",

    "description": "Adds a Tissue checkin button.",

    "icons": {
        "180": "icons/180.png"
    },

    "permissions": ["activeTab"],

    "background": {
        "scripts": ["script/background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_icon": "icons/180.png",
        "default_title": "Tissue Checkin"
    }
}
