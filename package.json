{
    "name": "tissue-checkin-extension",
    "version": "1.2.0",
    "license": "MIT",
    "scripts": {
        "build": "webpack --mode development",
        "build:production": "webpack --mode production",
        "release": "run-s build:production create-release-folder zip",
        "create-release-folder": "shx mkdir -p release",
        "zip": "cd dist && cross-conf-env bestzip ../release/npm_package_name_npm_package_version.zip *"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/plugin-transform-named-capturing-groups-regex": "^7.8.3",
        "@babel/preset-env": "^7.10.2",
        "@types/copy-webpack-plugin": "^6.0.0",
        "@types/webpack": "^4.41.16",
        "babel-loader": "^8.1.0",
        "bestzip": "^2.1.7",
        "copy-webpack-plugin": "^6.0.1",
        "cross-conf-env": "^1.1.2",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "shx": "^0.3.2",
        "stylelint": "^13.5.0",
        "stylelint-config-recess-order": "^2.0.4",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "webextension-polyfill-ts": "^0.16.0"
    },
    "volta": {
        "node": "16.20.1"
    }
}
