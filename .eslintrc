{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off"
    },
    "globals": { "chrome": "readonly", "browser": "readonly" }
}
